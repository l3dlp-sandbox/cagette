::use 'admin/design.mtt'::

		<h3>Stats mensuelles</h3>
		<!--<p>
			<a href="/admin/stats/::month-1::/::year::" class="btn btn-default btn-sm">Précédent</a>
			Du ::dDate(from):: au ::dDate(to)::
			<a href="/admin/stats/::month+1::/::year::" class="btn btn-default btn-sm">Suivant</a><br/>
		</p>-->

		$$timeframe(::tf::)
		
		<p>
			<h4>Paniers</h4>
			<table class="table table-bordered table-striped">				
				<tr>
					<td class="col-md-6">Nbre de paniers (MARKET)</td>
					<td class="col-md-6">::marketBasketsNum::</td>
				</tr>
				<tr>
					<td class="col-md-6">Nbre de paniers (AMAP)</td>
					<td class="col-md-6">::amapBasketsNum::</td>
				</tr>		
			</table>


			<h4>Producteurs</h4>
			<table class="table table-bordered table-striped">				
				<tr>
					<td class="col-md-6">Nouveaux producteurs</td>
					<td class="col-md-6">::newVendors::</td>
				</tr>	
				<tr>
					<td>Nouveaux producteurs par type</td>
					<td>
						<table class='table'>
							::foreach s newVendorsByType::
							<tr>
								<td>$$vendorType(::s.type::)</td>
								<td>::s.count::</td>
							</tr>
							::end::
						</table>
					</td>
				</tr>	
				<tr>
					<td>Producteurs actifs (maintenant)</td>
					<td>::activeVendors::</td>
				</tr>	
				<tr>
					<td>Producteurs actifs par type (maintenant)</td>
					<td>
						<table class='table'>
						::foreach s activeVendorsByType::
						<tr>							
							<td>$$vendorType(::s.type::)</td>
							<td>::s.count::</td>
						</tr>
						::end::
						</table>
					</td>
				</tr>		
            </table>
            
			<h4>Consommateurs</h4>

			<table class="table table-bordered table-striped">				
				<tr>
					<td class="col-md-6">Nouveaux groupes</td>
					<td class="col-md-6">::newGroups::</td>
				</tr>	
				<tr>
					<td>Nouveaux groupes par type</td>
					<td>

						<table class='table'>							
							<tr>
								<td>AMAPs</td>
								<td>::newCSAGroups::</td>
							</tr>
							<tr>
								<td>mode Marché</td>
								<td>::newMarketGroups::</td>
							</tr>
							
						</table>
					</td>
				</tr>
				<tr>
					<td>Nouveaux groupes par type d'administrateur</td>
					<td>
						<table class='table'>		
						::foreach g newGroupsByAdmin::
						<tr>							
							<td>::g.contactType::</td>
							<td>::g.count::</td>
						</tr>
						::end::
						</table>
					</td>
				</tr>
				
				<tr>
					<td>Groupes actifs (maintenant)</td>
					<td>
						::activeGroups::
					</td>
				</tr>	
				<tr>
					<td>Groupes actifs par type d'administrateur</td>
					<td>
						<table class='table'>		
							::foreach g activeGroupsByAdmin::
							<tr>							
								<td>::g.contactType::</td>
								<td>::g.count::</td>
							</tr>
							::end::
						</table>
					</td>
				</tr>	
				<tr>
					<td>Foyers actifs (maintenant)</td>
					<td>::activeUsers::</td>
				</tr>	
				<tr>
					<td>Nouveaux foyers</td>
					<td>::newUsers::</td>
				</tr>
			
            </table>
			
		</p>


::end::